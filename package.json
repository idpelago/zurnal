{
  "name": "zurnal",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pull": "git pull --rebase && yarn reload",
    "reload": "yarn build && pm2 reload zurnal",
    "restart": "yarn build && pm2 restart zurnal",
    "prod": "pm2 start pm2.json -i 2 --wait-ready --listen-timeout 10000",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "next": "12.0.7",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.2",
    "sass": "^1.45.0"
  },
  "devDependencies": {
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7"
  }
}